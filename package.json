{
    "name": "weaver-front",
    "private": true,
    "description": "weaver-front",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install",
        "dev": "vite",
        "build": "tsc && vite build --mode prod",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint src --ext ts,tsx --fix",
        "lint:lint-staged": "lint-staged",
        "preview": "vite preview",
        "test": "vitest",
        "test:staged": "vitest --run",
        "cy:open": "cypress open",
        "coverage": "vitest run --coverage",
        "release": "release-it"
    },
    "lint-staged": {
        "*.{jsx,ts,tsx}": [
            "pnpm run lint",
            "npx prettier --write"
        ],
        "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
            "npx prettier --write--parser json"
        ],
        "package.json": [
            "npx prettier --write"
        ],
        "*.{scss,less,styl,html}": [
            "npx prettier --write"
        ],
        "*.md": [
            "npx prettier --write"
        ]
    },
    "keywords": [
        "React",
        "TypeScript",
        "Vite"
    ],
    "author": "zhangzhouou@gmail.com",
    "dependencies": {
        "@arco-design/web-react": "^2.66.1",
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@supuwoerc/utils": "^1.1.8",
        "@tanstack/react-query": "^4.29.12",
        "@tanstack/react-query-devtools": "^4.29.14",
        "@types/lodash-es": "^4.17.12",
        "await-to-js": "^3.0.0",
        "axios": "^1.6.8",
        "classnames": "^2.5.1",
        "dayjs": "^1.11.8",
        "emotion-reset": "^3.0.1",
        "eventemitter3": "^5.0.1",
        "immer": "^10.0.2",
        "lodash-es": "^4.17.21",
        "lucide-react": "^0.447.0",
        "md5": "^2.3.0",
        "motion": "^12.23.12",
        "nanoid": "^5.0.7",
        "nprogress": "^0.2.0",
        "nuqs": "^2.3.2",
        "p-limit": "^6.2.0",
        "path-browserify": "^1.0.1",
        "qs": "^6.11.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-easy-crop": "^5.5.0",
        "react-error-boundary": "^4.0.9",
        "react-image-crop": "^11.0.10",
        "react-intl": "^6.6.6",
        "react-router-dom": "^6.12.0",
        "react-transition-group": "^4.4.5",
        "react-use": "^17.5.0",
        "swapy": "^0.1.2",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@arco-plugins/vite-react": "^1.3.3",
        "@commitlint/cli": "^17.6.5",
        "@commitlint/config-conventional": "^17.6.5",
        "@emotion/babel-plugin": "^11.11.0",
        "@release-it/conventional-changelog": "^8.0.1",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.4.8",
        "@testing-library/react": "^16.0.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.12",
        "@types/md5": "^2.3.5",
        "@types/node": "^20.2.5",
        "@types/nprogress": "^0.2.3",
        "@types/path-browserify": "^1.0.3",
        "@types/qs": "^6.9.7",
        "@types/react": "^18.0.37",
        "@types/react-dom": "^18.0.11",
        "@types/react-transition-group": "^4.4.6",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "@vitejs/plugin-react": "^4.0.0",
        "@vitest/coverage-v8": "^2.0.5",
        "cypress": "^13.13.2",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-jest-dom": "^5.4.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.3.4",
        "husky": "^8.0.3",
        "jsdom": "^24.1.1",
        "less": "^4.2.0",
        "lint-staged": "^13.2.2",
        "prettier": "^2.8.8",
        "react-scan": "^0.1.3",
        "release-it": "^17.6.0",
        "terser": "^5.17.7",
        "type-fest": "^3.11.1",
        "typescript": "~5.8.3",
        "vite": "^5.3.5",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-svgr": "^4.2.0",
        "vitest": "^2.0.5"
    },
    "license": "ISC",
    "engines": {
        "node": "22.16.0",
        "pnpm": "10.11.0"
    },
    "volta": {
        "node": "22.16.0"
    }
}
